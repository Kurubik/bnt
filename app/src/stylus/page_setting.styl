body
  -webkit-tap-highlight-color rgba(0,0,0,0)
  min-width 1200px
  line-height 1.25
  font-size 12px
  color basecolor
  font-family reg, Arial, Helvetica, sans-serif


body,html
  height 100%

a
  outline none
  text-decoration none
  color inherit


h1,h2,h3,h4
  font-weight 100

strong
  font-family bold

.content
  position relative
  min-height 100%
  background-color #313130
  &.home
    background-color #313130
    background-image url('../images/air.png')
    background-repeat no-repeat
    background-position: center -262px


.wrapper
  max-width 1200px
  margin 0 auto
  position relative


.p_inner
  position relative
  &.top_style
    padding-top 137px

.inner_content
  margin 0 100px

.block-B
  position relative
  top -137px
  height 470px
  margin-right 100px
  background-color #000000
  opacity 0.4
